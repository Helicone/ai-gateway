{
    "name": "@helicone/helix",
    "version": "0.1.0-beta.1",
    "description": "A high-performance proxy router for LLM APIs built with Rust",
    "main": "./bin/index.js",
    "bin": {
        "helicone-router": "./bin/index.js"
    },
    "files": [
        "bin",
        "dist"
    ],
    "engines": {
        "node": ">=16"
    },
    "scripts": {
        "test": "node test-local.js",
        "prepublishOnly": "npm test",
        "publish:dry": "npm publish --dry-run",
        "clean": "rm -rf dist/*",
        "clean:docker": "docker system prune -f && docker volume prune -f",
        "size-check": "ls -lh dist/ && du -sh dist/"
    },
    "keywords": [
        "llm",
        "proxy",
        "router",
        "api",
        "rust",
        "cli"
    ],
    "author": "Helicone",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Helicone/helicone-router.git",
        "directory": "npx"
    },
    "bugs": {
        "url": "https://github.com/Helicone/helicone-router/issues"
    },
    "homepage": "https://github.com/Helicone/helicone-router#readme",
    "preferGlobal": false,
    "os": [
        "darwin",
        "linux"
    ]
}